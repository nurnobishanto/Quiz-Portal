{
    "name": "awcodes/filament-quick-create",
    "type": "package",
    "description": "Plugin for Filament Admin that adds a dropdown menu to the header to quickly create new items.",
    "keywords": [
        "framework",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "laravel/pint": "^1.2",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "FilamentQuickCreate\\": "src/",
            "FilamentQuickCreate\\Database\\Factories\\": "database/factories/",
            "FilamentQuickCreate\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentQuickCreate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentQuickCreate\\FilamentQuickCreateServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "pint": "pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
